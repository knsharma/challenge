#include <stdio.h>
#include <conio.h>
#include <iostream>
#define MIN(x,y) (x < y ? x : y) 
#define MAX(x,y) (x > y ? x : y) 
#define INSIDE 0 
#define OUTSIDE 1

class polygoncheck
{
    typedef struct
    {
        double x,y;
        
    } Point;
    
    void main(int n, Point *s, Point p)
    {
        int c=0;
        double inters;
        Point p1, p2;
        p1=p;
        for(int i=1; i<=n; i++)
        {
            p2=s[i%n];
            if (p.y > MIN(p1.y,p2.y))
            {
                if (p.y <= MAX(p1.y,p2.y))
                {
                    if (p.x <= MAX(p1.x,p2.x))
                    {
                        if (p1.y != p2.y)
                        {
                            inters = (p.y-p1.y)*(p2.x-p1.x)/(p2.y-p1.y)+p1.x;
                            if (p1.x == p2.x || p.x <= inters)
                            c++;
                            
                        }
                        
                    }
                    
                }
                
            }
            
        }
        p1 = p2;
        if(c==0)
        {
            std::cout << "OUTSIDE" << std::endl;
            
        }
        else if(c%2==0)
        {
            std::cout << "OUTSIDE" << std::endl;
            
        }
        else
        {
            std::cout << "INSIDE" << std::endl;
            
        }
        
    }
    
};
